<h1>Users</h1>
<table>
  <head>
    <tr>
      <th>image</th>
      <th>name</th>
    </tr>
    <tr>
    <% @users.each do |user| %>
      <th><%= image_tag user.get_profile_image(100,100) %></th>
      <th><%= user.name %></th>
      <th><%= link_to 'Show', user_path(user.id) %></th>
    </tr>
    <% end %>
  </head>
</table>